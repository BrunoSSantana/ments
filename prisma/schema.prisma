// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  name      String
  password  String
  email     String
  github    String
  linkedin  String?
  mentoreds User[]   @relation("UserToUser")
  m_max     Int?
  about     String
  field     String
  languages String[]
  User      User?    @relation("UserToUser", fields: [userIds], references: [id])
  userIds   String[]
}
